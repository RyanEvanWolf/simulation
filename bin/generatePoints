#!/usr/bin/env python



import pickle
import os
import rospy

import time

from bumblebee.stereo import stereoCamera
from simulation.path import *
from simulation.settings import *
from simulation.dataset import idealSimulator 
import tf.transformations
from geometry_msgs.msg import TransformStamped,Transform,Quaternion,Vector3

rospy.init_node('genPoints')

lSettings=getSimulatedLandmarkSettings()

bumblebee=stereoCamera()

sim=idealSimulator(lSettings,bumblebee.kSettings,400)

sim.run("/media/ryan/EXTRA/Simulation/Motion")


sim.dumpTo("/media/ryan/EXTRA/Simulation/Landmarks")

sim.publishLandmarks()

rospy.spin()

